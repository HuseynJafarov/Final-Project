@model Social

@{
    ViewData["Title"] = "Detail";
    Layout = "~/Areas/AdminArea/Views/Shared/_AdminLayout.cshtml";
}

<div class="container my-5">
    <div class="card">
        <div class="card-header">
            Social detail
        </div>
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>
                        Name
                    </th>
                    <th>
                        Icon
                    </th>

                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>
                        @Html.Raw(Model.Name)
                    </td>
                    <td>
                        @Html.Raw(Model.Icon)
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
</div>
